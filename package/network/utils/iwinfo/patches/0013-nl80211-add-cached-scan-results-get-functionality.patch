+static int nl80211_get_scan_results(const char *ifname, char *buf, int *len)
+{
+	struct nl80211_scanlist sl = { .e = (struct iwinfo_scanlist_entry *)buf };
+	char *res = NULL;
+	int rv;
+
+	*len = 0;
+	
+	if (!strncmp(ifname, "radio", 5)) {
+		if ((res = nl80211_phy2ifname(ifname)) != NULL) {
+			return nl80211_get_scan_results(res, buf, len);
+		} else if ((res = nl80211_ifadd(ifname)) != NULL) {
+			rv = nl80211_get_scan_results(res, buf, len);
+			nl80211_ifdel(res);
+			return rv;
+		}
+	}
+
+	if (nl80211_request(ifname, NL80211_CMD_GET_SCAN, NLM_F_DUMP,
+						nl80211_get_scanlist_cb, &sl))
+		goto out;
+
+	*len = sl.len * sizeof(struct iwinfo_scanlist_entry);
+	return 0;
+
+out:
+	*len = 0;
+	return -1;
+}
