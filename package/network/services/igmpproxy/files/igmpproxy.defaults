#!/bin/sh

. /lib/functions.sh

index=0

config_cb() {
	[ "$1" = "phyint" ] || return
	[ "${2:0:3}" != "cfg" ] && return

	index=$((index + 1))

	while uci_get "igmpproxy" "$index" >/dev/null; do
		index=$((index + 1))
	done

	uci_rename "igmpproxy" "$2" "$index"
}

config_load "igmpproxy"
uci_commit "igmpproxy"

exit 0
