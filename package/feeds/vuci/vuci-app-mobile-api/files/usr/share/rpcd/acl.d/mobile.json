{
  "network/mobile/general": {
    "description": "mobile general configuration",
    "read": {
      "api": ["/modems/*", "/sim_cards/*"]
    },
    "write": {
      "api": ["/modems/*", "/sim_cards/*"]
    }
  },

  "network/mobile/connection": {
    "description": "mobile connection configuration",
    "write": {
      "api": ["/modems/*", "/simcards/*"]
    },
    "read": {
      "api": ["/modems/*", "/simcards/*"]
    }
  },

  "network/mobile/sim_switch": {
    "description": "mobile sim switch configuration",
    "read": {
      "api": ["/modems/*", "/sim_cards/*", "/sim_switch/*", "/data_limit/config/*"]
    },
    "write": {
      "api": ["/modems/*", "/sim_cards/*", "/sim_switch/*"]
    }
  },

  "network/mobile/operators/scan": {
    "description": "mobile operators configuration",
    "read": {
      "api": ["/modems/*", "/sim_cards/*", "/operator_lists/*"]
    },
    "write": {
      "api": ["/modems/*", "/sim_cards/*", "/operator_lists/*"]
    }
  },

  "network/mobile/operators/list": {
    "description": "mobile operators configuration",
    "read": {
      "api": ["/modems/*", "/operator_lists/*", "/apn_database/*"]
    },
    "write": {
      "api": ["/modems/*", "/operator_lists/*"]
    }
  },

  "network/mobile/utilities": {
    "description": "mobile utilities configuration",
    "write": {
      "api": ["/modems/*", "/simcards/*"]
    },
    "read": {
      "api": ["/modems/*", "/simcards/*"]
    }
  },

  "network/mobile/apn_database": {
    "read": {
      "api": ["/apn_database/*"]
    },
    "write": {
      "api": ["/apn_database/*"]
    }
  },

  "network/mobile/limits/*": {
    "description": "mobile limits configuration",
    "write": {
      "api": ["/modems/*", "/simcards/*", "/data_limit/*"]
    },
    "read": {
      "api": ["/modems/*", "/simcards/*", "/data_limit/*"]
    }
  },

  "network/mobile/esim_profiles": {
    "read": {
      "api": ["/esim/*"]
    },
    "write": {
      "api": ["/esim/*"]
    }
  }
}
