                    "status": {
                      "type": "string"
                    },
                    "msd_status": {
                      "type": "string"
                    },
                    "mmc_status": {
                      "type": "string"
                    },
                    "checked": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "200memoryinfo": {
        "description": "Returns section",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "description": "Operation status",
                  "type": "boolean"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "msd_status": {
                      "type": "string"
                    },
                    "mmc_status": {
                      "type": "string"
                    },
