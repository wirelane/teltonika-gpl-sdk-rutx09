    "schemas": {
      "tinc_host_options": {
        "allOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "x-web": {
                  "pretty": "Tinc host configuration name",
                  "unique": true,
                  "input-type": "input"
                },
                "type": "string",
                "description": "Tinc Host Configuration identifier (ID)."
              }
            },
            "required": [
              "id"
            ]
          },
          {
            "$ref": "#/components/schemas/tinc_host_single_options"
          }
        ]
      },
      "tinc_host_single_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": "Enabled",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enables/disables Tinc Host instance.",
            "format": "is_bool",
            "enum": [
              "0",
              "1"
            ]
          },
          "address": {
            "x-web": {
              "pretty": "Address",
              "input-type": "input"
            },
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "It must resolve to the external IP address where the host can be reached, not the one that is internal to the VPN.",
            "format": "host"
          },
          "subnet": {
            "x-web": {
              "pretty": "Subnet",
              "input-type": "input"
            },
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "The subnet which this tinc daemon will serve. Multiple subnet lines can be specified for each Daemon.",
            "format": "ipmask"
          },
          "publickeyfile": {
            "x-web": {
              "pretty": "Public Key",
              "input-type": "input"
            },
            "type": "string",
            "format": "binary",
            "description": "RSA public key generated by host."
          }
        }
      }
    }
