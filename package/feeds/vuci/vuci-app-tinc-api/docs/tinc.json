    "schemas": {
      "tinc_options": {
        "allOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "x-web": {
                  "pretty": "Configuration Name",
                  "input-type": "input"
                },
                "type": "string",
                "description": "Tinc Configuration identifier (ID)."
              }
            },
            "required": [
              "id"
            ]
          },
          {
            "$ref": "#/components/schemas/tinc_single_options"
          }
        ]
      },
      "tinc_single_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": "Enabled",
              "input-type": "switch",
              "group": "General Setup"
            },
            "type": "string",
            "description": "Turn Tinc VPN on or off.",
            "format": "is_bool",
            "enum": [
              "0",
              "1"
            ]
          },
          "addressfamily": {
            "x-web": {
              "pretty": "Address Family",
              "input-type": "select",
              "group": "Advanced Settings",
              "enum-pretty": {
                "any": "ANY",
                "ipv4": "IPv4",
                "ipv6": "IPv6"
              }
            },
            "type": "string",
            "description": "This option affects the address family of listening and outgoing sockets.",
            "enum": [
              "any",
              "ipv4",
              "ipv6"
            ]
          },
          "bindtoaddress": {
            "x-web": {
              "pretty": "Bind To Address",
              "input-type": "input",
              "group": "Advanced Settings"
            },
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Addresses for Tinc VPN to use for listening sockets.",
            "format": "host"
          },
          "bindtointerface": {
            "x-web": {
              "pretty": "Bind To Interface",
              "input-type": "input",
              "group": "Advanced Settings"
            },
            "type": "string",
            "description": "Interface to listen for incoming connections."
          },
          "connectto": {
            "x-web": {
              "pretty": "Host to Connect to",
              "input-type": "input",
              "group": "General Setup"
            },
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Specifies which other tinc daemon to connect to on startup."
          },
          "keyexpire": {
            "x-web": {
              "pretty": "Key Expire",
              "input-type": "input",
              "group": "Advanced Settings"
            },
            "type": "string",
            "description": "This option controls the time the encryption keys used to encrypt the data are Valid.",
            "format": "range(1, 90000000)",
            "minLength": 1,
            "maxLength": 8
          },
          "mode": {
            "x-web": {
              "pretty": "Mode",
              "input-type": "select",
              "group": "Advanced Settings",
              "enum-pretty": {
                "router": "Router",
                "switch": "Switch",
                "hub": "Hub"
              }
            },
            "type": "string",
            "description": "This option selects the way packets are routed to other daemons.",
            "enum": [
              "router",
              "switch",
              "hub"
            ]
          },
          "subnet": {
            "x-web": {
              "pretty": "Subnet",
              "input-type": "input",
              "group": "General Setup"
            },
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "The subnet which this tinc daemon will serve. Multiple subnet lines can be specified for each Daemon.",
            "format": "ipmask"
          },
          "pinginterval": {
            "x-web": {
              "pretty": "Ping Interval",
              "input-type": "input",
              "group": "Advanced Settings"
            },
            "type": "string",
            "description": "The number of seconds of inactivity that tinc will wait before sending a probe to the other end.",
            "format": "range(1, 86400)",
            "minLength": 1,
            "maxLength": 5
          },
          "pingtimeout": {
            "x-web": {
              "pretty": "Ping Timeout",
              "input-type": "input",
              "group": "Advanced Settings"
            },
            "type": "string",
            "description": "The number of seconds to wait for a response to pings or to allow meta connections to block.",
            "format": "range(1, 86400)",
            "minLength": 1,
            "maxLength": 5
          },
          "privatekeyfile": {
            "x-web": {
              "pretty": "Private Key",
              "input-type": "input",
              "group": "General Setup"
            },
            "type": "string",
            "format": "binary",
            "description": "The full path name of the RSA private key file that was generated."
          },
          "publickeyfile": {
            "x-web": {
              "pretty": "Public Key",
              "input-type": "input",
              "group": "General Setup"
            },
            "type": "string",
            "format": "binary",
            "description": "The full path name of the RSA public key file that was generated."
          }
        }
      }
