            "type": "string",
            "enum": [
              "0",
              "1"
            ],
            "default": "0",
            "description": "Enable wifi scanner for the 2.4GHz wireless device",
            "x-web": {
              "pretty": "Enable 2.4Ghz",
              "input-type": "switch",
              "hw_tags": { "wifi": true }
            }
          },
          "five_g_enabled": {
            "type": "string",
            "enum": [
              "0",
              "1"
            ],
            "default": "0",
            "description": "Enable wifi scanner for the 5GHz wireless device",
            "x-web": {
              "pretty": "Enable 5GHz",
              "input-type": "switch",
              "hw_tags": { "wifi5Ghz": true }
            }
          },
          "interval": {
            "type": "string",
            "description": "Interval between wifi scans.",
            "default": "10",
            "format": "range(1, 2147483)",
            "x-web": {
              "pretty": "Interval",
              "input-type": "input"
            }
