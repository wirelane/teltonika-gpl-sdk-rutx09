{
  "openapi": "3.0.0",
  "tags": [],
  "paths": {
    "/routes/dynamic/bgp/general/config/general": {
      "get": {
        "tags": [
          "bgp_general"
        ],
        "summary": "Returns bgp general configuration",
        "description": "",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200BgGe"
          }
        }
      },
      "put": {
        "tags": [
          "bgp_general"
        ],
        "summary": "Updates bgp general configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200BgGe"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "description": "Parameter section",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "$ref": "#/components/schemas/bgp_general_options"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Upload config",
        "operationId": "post-custom-bgp-config_id",
        "responses": {
          "200": {
            "$ref": "#/components/responses/file_response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/bgpd_custom_conf"
        },
        "tags": [
          "DNS"
        ]
      },
      "parameters": [],
      "x-web": {
        "menu": [
          "Network",
          "Routing",
          "Dynamic Routes",
          "BGP"
        ],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false
        }
      }
    },
    "/routes/dynamic/bgp/general/bgp_info": {
      "get": {
        "summary": "Fetches data about bgp neighbors",
        "tags": [
          "bgp_general"
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/200response"
          }
        },
        "operationId": "get-network-routes-dynamic-bgp-general-actions-bgp_info"
      },
      "parameters": [],
      "x-web": {
        "menu": [
          "Network",
          "Routing",
          "Dynamic Routes",
          "BGP"
        ],
        "hw_tags": {
          "is_access_point": false
        }
      }
    }
  },
  "components": {
    "responses": {
      "200BgGe": {
        "description": "Returns section",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "$ref": "#/components/schemas/bgp_general_options"
                }
              }
            }
          }
        }
      },
      "200BgGeMulti": {
        "description": "Returns sections",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "array",
                  "description": "Array of section objects",
                  "items": {
                    "$ref": "#/components/schemas/bgp_general_options"
                  }
                }
              }
            }
          }
        }
      },
      "file_response": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "path": {
                      "type": "string"
                    }
                  }
                }
              },
              "x-examples": {
                "example-1": {
                  "success": true,
                  "data": {
                    "path": "/etc/vuci-uploads/cbid.file.id.parameter.file_name.ext"
                  }
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "bgp_general_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enable/Disable BGP protocol."
          },
          "enabled_vty": {
            "x-web": {
              "pretty": "Enable VTY",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enable/Disable vty access from LAN."
          },
          "bgpd_custom_conf": {
            "x-web": {
              "pretty": "Import config",
              "input-type": "input"
            },
            "type": "string",
            "items": {
              "type": "string"
            },
            "format": "binary",
            "description": "Use imported BGP configuration."
          },
          "bgpd_custom_conf:file_size": {
            "description": "Uploaded file size in bytes.",
            "type": "number",
            "readOnly": true
          }
        }
      }
    },
    "requestBodies": {
      "bgpd_custom_conf": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "option": {
                  "type": "string",
                  "description": "Parameter to upload the file to.",
                  "enum": [
                    "bgpd_custom_conf"
                  ]
                },
                "file": {
                  "type": "string",
                  "format": "binary",
                  "description": "File to upload."
                }
              },
              "required": [
                "option",
                "file"
              ]
            }
          }
        }
      }
    }
  }
}