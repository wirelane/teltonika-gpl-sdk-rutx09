{
  "network/dhcp_servers/static_leases": {
    "description": "",
    "read": {
      "api": ["/dhcp/static_leases/*",
			  "/dhcp/servers/ipv4/config/*",
			  "/dhcp/leases/ipv4/status/*",
			  "/dhcp/leases/ipv6/status/*",
			  "/interfaces/basic/status/*",
			  "/routes/status/*"]
    },
    "write": {
      "api": ["/dhcp/static_leases/*"]
    }
  },
  "network/dhcp_servers/general": {
    "description": "",
    "read": {
      "api": ["/dhcp/servers/*", "/interfaces/config/*", "/interfaces/basic/status/*"]
    },
    "write": {
      "api": ["/dhcp/servers/*"]
    }
  },
  "status/network/dhcp_leases": {
    "description": "",
    "read": {
      "api": ["/dhcp/*", "/routes/status/*"]
    },
    "write": {
      "api": ["/dhcp/*"]
    }
  },
  "status/network/lan": {
    "description": "lan network status",
    "read": {
      "api": ["/interfaces/basic/status/*", "/dhcp/*"]
    }
  }
}
