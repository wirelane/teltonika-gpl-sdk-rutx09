  "components": {
    "schemas": {
      "ulog": {
        "description": "",
        "type": "object",
        "properties": {
          "enabled": {
            "type": "string",
            "description": "Enable traffic logging",
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            }
          },
          "network": {
            "type": "array",
            "description": "Networks",
            "enum": [
              "lan",
              "hotspot"
            ],
            "items": {
              "type": "string"
            },
            "x-web": {
              "pretty": "Networks",
              "input-type": "select"
            }
          }
        },
        "required": [
          "enabled",
          "network"
        ]
      },
      "ulog_ftp": {
        "description": "",
        "type": "object",
        "properties": {
          "host": {
            "type": "string",
            "description": "The domain name or IP address of the server",
            "format": "host",
            "x-web": {
              "pretty": "Server address",
              "input-type": "input",
              "group": "FTP Server"
            }
          },
          "username": {
            "type": "string",
            "description": "The user name of the FTP server that will be used for logs uploading",
            "format": "credentials_validate",
            "x-web": {
              "pretty": "Username",
              "input-type": "input",
              "group": "FTP Server"
            }
          },
          "password": {
            "type": "string",
            "description": "The password of the FTP server that will be used for logs uploading",
            "format": "credentials_validate",
            "writeOnly": true,
            "x-web": {
              "pretty": "Password",
              "input-type": "input",
              "group": "FTP Server"
            }
          },
          "port": {
            "type": "string",
            "description": "The TCP/IP port of the server",
            "format": "port",
            "default": "21",
            "x-web": {
              "pretty": "Port",
              "input-type": "input",
              "group": "FTP Server"
            }
          },
          "remote_file_path": {
            "type": "string",
            "description": "FTP server's subfolder in which received file will be stored. Entry should end with a slash (either \"\/\" or \"\\\")",
            "format": "string",
            "x-web": {
              "pretty": "Subfolder",
              "input-type": "input",
              "group": "FTP Server"
            }
          },
          "extra_name_info": {
            "type": "string",
            "description": "Extra information to be added to file name",
            "enum": [
              "none",
              "mac",
              "serial",
              "custom"
            ],
            "x-web": {
              "pretty": "File name extras",
              "input-type": "select",
              "group": "Upload Settings",
              "enum-pretty": {
                "none": "No extra information",
                "mac": "MAC address",
                "serial": "Serial number",
                "custom": "Custom string"
              }
            }
          },
          "custom_string": {
            "type": "string",
            "description": "Custom string to be added to ftp file name",
            "format": "regex",
            "pattern": "^[a-zA-Z0-9_+.-]+$",
            "x-web": {
              "pretty": "Custom string",
              "input-type": "input",
              "group": "Upload Settings",
              "depends": [
                {
                  "extra_name_info": "custom"
                }
              ]
            }
          },
          "fixed": {
            "type": "string",
            "description": "The schedule mode to be used for uploading to FTP server",
            "enum": [
              "0",
              "1"
            ],
            "x-web": {
              "pretty": "Mode",
              "input-type": "select",
              "group": "Upload Settings",
              "enum-pretty": {
                "0": "Interval",
                "1": "Fixed"
              }
            }
          },
          "hours": {
            "type": "string",
            "description": "Uploading will be performed on this specific time of the day. Range [0 - 23]",
            "format": "irange(0,23)",
            "x-web": {
              "pretty": "Hours",
              "input-type": "input",
              "group": "Upload Settings",
              "depends": [
                {
                  "fixed": "1"
                }
              ]
            }
          },
          "minutes": {
            "type": "string",
            "description": "Uploading will be performed on this specific time of the day. Range [0 - 59]",
            "format": "irange(0,59)",
            "x-web": {
              "pretty": "Minutes",
              "input-type": "input",
              "group": "Upload Settings",
              "depends": [
                {
                  "fixed": "1"
                }
              ]
            }
          },
          "interval": {
            "type": "string",
            "description": "Upload logs to server every x hours",
            "default": "1",
            "enum": [
              "1",
              "2",
              "4",
              "8",
              "12",
              "24"
            ],
            "x-web": {
              "pretty": "Upload interval",
              "input-type": "select",
              "group": "Upload Settings",
              "depends": [
                {
                  "fixed": "0"
                }
              ],
              "enum-pretty": {
                "1": "1 hour",
                "2": "2 hours",
                "4": "4 hours",
                "8": "8 hours",
                "12": "12 hours",
                "24": "24 hours"
              }
            }
          },
          "weekdays": {
            "type": "array",
            "description": "Uploading will be performed on these days only",
            "items": {
              "type": "string",
              "enum": [
                "mon",
                "tue",
                "wed",
                "thu",
                "fri",
                "sat",
                "sun"
              ]
            },
            "x-web": {
              "pretty": "Days",
              "input-type": "select",
              "group": "Upload Settings",
              "enum-pretty": {
                "mon": "Monday",
                "tue": "Tuesday",
                "wed": "Wednesday",
                "thu": "Thursday",
                "fri": "Friday",
                "sat": "Saturday",
                "sun": "Sunday"
              }
            }
          }
        },
        "required": [
          "weekdays"
        ]
      },
      "ulog_status": {
        "description": "",
        "type": "object",
        "x-examples": {
          "example-1": {
