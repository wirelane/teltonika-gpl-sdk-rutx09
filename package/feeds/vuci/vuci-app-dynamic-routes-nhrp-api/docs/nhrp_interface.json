    },
    "schemas": {
      "nhrp_interface_options": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "enabled": {
            "type": "string",
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            },
            "description": "Enables client.",
            "enum": [
              "0",
              "1"
            ]
          },
          "interface": {
            "x-web": {
              "pretty": "Interface",
              "input-type": "input"
            },
            "type": "string",
            "description": "Interface which will be using NHRP."
          },
          "network_id": {
            "type": "string",
            "x-web": {
              "pretty": "Network ID",
              "input-type": "input"
            },
            "description": "Network ID of NHRP.",
            "format": "irange(1, 4294967295)"
          },
          "auth": {
            "type": "string",
            "x-web": {
              "pretty": "NHRP authentication key",
              "input-type": "input"
            },
            "description": "NHRP authentication key",
            "maxLength": 8
          },
          "proto_address": {
            "x-web": {
              "pretty": "NHS",
              "input-type": "input"
            },
            "type": "string",
            "description": "IP address of Next-Hop Server. NHS must be an IP address or 'dynamic'."
          },
          "nbma_address": {
            "type": "string",
            "x-web": {
              "pretty": "NBMA",
              "input-type": "input"
            },
            "description": "Non-Broadcast Multi-Access(NBMA) network IP address."
          },
          "holdtime": {
            "type": "string",
            "x-web": {
              "pretty": "Hold-time",
              "input-type": "input"
            },
            "description": "Specifies the holding time for NHRP Registration Requests and Resolution Replies sent from this interface or shortcut-target. The holdtime is specified in seconds and defaults to two hours.",
            "format": "irange(1, 65000)"
          },
          "ipsec_support": {
            "type": "string",
            "x-web": {
              "pretty": "IPsec support",
              "input-type": "switch"
            },
            "description": "Use NHRP over IPsec.",
            "enum": [
              "0",
              "1"
            ]
          },
          "ipsec_instance": {
            "x-web": {
              "pretty": "IPsec instance",
              "input-type": "input",
              "depends": [
                {
                  "ipsec_support": "1"
                }
              ]
            },
            "type": "string",
            "description": "Select IPsec instance name."
          }
        }
      }
    }
