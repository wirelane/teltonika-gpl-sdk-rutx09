{
  "system/wizard/step_pwd": {
    "description": "setup wizard general",
    "read": {
      "api": {
        "/date_time/ntp/client/*": ["*"],
        "/system/languages/options/*": ["*"],
        "/system/config/*": ["*"]
      }
    },
    "write": {
      "api": ["/date_time/ntp/client/*", "/system/config/*"]
    }
  },
  "system/wizard/step_lan": {
    "description": "setup wizard lan",
    "read": {
      "api": {
        "/modems/status/*": ["builtin"],
        "/interfaces/config/*": ["*"],
        "/dhcp/servers/ipv4/config/*": ["*"],
        "/dhcp/servers/ipv6/config/*": ["*"]
      }
    },
    "write": {
      "api": ["/interfaces/config/*", "/dhcp/servers/ipv4/config/*", "/dhcp/servers/ipv6/config/*"]
    }
  },
  "system/wizard/step_wan": {
    "description": "setup wizard wan",
    "read": {
      "api": {
        "/modems/status/*": ["*"],
        "/modems/apns/status/*": ["*"],
        "/interfaces/config/*": ["*"],
        "/interfaces/basic/status/*": ["*"],
        "/sim_cards/config/*": ["*"]
      }
    },
    "write": {
      "api": ["/interfaces/config/*", "/sim_cards/config/*"]
    }
  },
  "system/wizard/step_wifi": {
    "description": "setup wizard wifi",
    "read": {
      "api": {
        "/wireless/interfaces/config/*": ["*"],
        "/modems/status/*": ["builtin"]
      }
    },
    "write": {
      "api": ["/wireless/interfaces/config/*"]
    }
  },
  "system/wizard/step_rms": {
    "description": "setup wizard rms",
    "read": {
      "api": {
        "/rms/status/*": ["*"],
        "/modems/status/*": ["builtin"]
      }
    },
    "write": {
      "api": ["/rms/*"]
    }
  }
}
