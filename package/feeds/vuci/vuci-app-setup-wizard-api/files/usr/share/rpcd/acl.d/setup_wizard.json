{
    "system/wizard/step_pwd": {
        "description": "setup wizard general",
        "read": {
            "api": [
                "/date_time/ntp/client/*",
                "/package_manager/language_packages/*",
                "/system/languages/*",
                "/system/config/*"
            ]
        },
        "write": {
            "api": [
                "/date_time/ntp/client/*",
                "/system/config/*"
            ]
        }
    },
    "system/wizard/step_lan": {
        "description": "setup wizard lan",
        "read": {
            "api": [
                "/modems/status*",
                "/interfaces/config/*",
                "/dhcp/servers/*"
            ]
        },
        "write": {
            "api": [
                "/interfaces/config/*",
                "/dhcp/servers/*"
            ]
        }
    },
    "system/wizard/step_wan": {
        "description": "setup wizard wan",
        "read": {
            "api": [
                "/modems/status*",
                "/interfaces/*",
                "/sim_cards/*"
            ]
        },
        "write": {
            "api": [
                "/interfaces/*",
                "/sim_cards/*"
            ]
        }
    },
    "system/wizard/step_wifi": {
        "description": "setup wizard wifi",
        "read": {
            "api": [
                "/wireless/*",
                "/modems/status*"
            ]
        },
        "write": {
            "api": [
                "/wireless/interfaces/*"
            ]
        }
    },
    "system/wizard/step_rms": {
        "description": "setup wizard rms",
        "read": {
            "api": [
                "/rms/*",
                "/modems/status*"
            ]
        },
        "write": {
            "api": [
                "/rms/*"
            ]
        }
    }
}