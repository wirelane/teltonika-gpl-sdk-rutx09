    "schemas": {
      "ospf_interface_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enable."
          },
          "cost": {
            "x-web": {
              "pretty": "Cost",
              "input-type": "input"
            },
            "type": "string",
            "description": "The cost value is set to router-LSAâ€™s metric field and used for SPF calculation."
          },
          "hello_interval": {
            "x-web": {
              "pretty": "Hello Interval",
              "input-type": "input"
            },
            "type": "string",
            "description": "This value controls how frequently (every n seconds) a 'Hello' packet is sent out on the specified interface."
          },
          "dead_interval": {
            "x-web": {
              "pretty": "Dead Interval",
              "input-type": "input"
            },
            "type": "string",
            "description": "This value must be the same for all routers attached to a common network."
          },
          "retransmit_interval": {
            "x-web": {
              "pretty": "Retransmit Interval",
              "input-type": "input"
            },
            "type": "string",
            "description": "This value is used when retransmitting Database Description and Link State Request packets."
          },
          "priority": {
            "x-web": {
              "pretty": "Priority",
              "input-type": "input"
            },
            "type": "string",
            "description": "The router with the highest priority will be more eligible to become Designated Router. Setting the value to 0, makes the router ineligible to become Designated Router."
          },
          "type": {
            "x-web": {
              "pretty": "Type",
              "input-type": "select",
              "enum-pretty": {
                "broadcast": "Broadcast",
                "non-broadcast": "Non-Broadcast",
                "point-to-point": "Point-to-Point",
                "point-to-multipoint": "Point-to-Multipoint"
              }
            },
            "type": "string",
            "enum": [
              "broadcast",
              "non-broadcast",
              "point-to-point",
              "point-to-multipoint"
            ],
            "description": "You can choose different type of configuration."
          },
          "ptp_dmvpn": {
            "x-web": {
              "input-type": "input",
              "pretty": "Enable DMVPN",
              "input-type": "switch",
              "depends": [
                {
                  "type": "point-to-point"
                }
              ]
            },
            "type": "switch"
          },
          "authentication": {
            "x-web": {
              "pretty": "Authentication",
              "input-type": "select",
              "enum-pretty": {
                "none": "None",
                "pass": "Password",
                "md5_hmac": "MD5 HMAC"
              }
            },
            "type": "string",
            "enum": [
              "none",
              "pass",
              "md5_hmac"
            ],
            "description": "Connection authentification methods."
          },
          "router_id": {
            "x-web": {
              "input-type": "input",
              "pretty": "ID",
              "depends": [
                {
                  "authentication": "md5_hmac"
                }
              ]
            },
            "type": "string"
          },
          "password": {
            "x-web": {
              "input-type": "input",
              "pretty": "Password",
              "depends": [
                {
                  "authentication": "md5_hmac"
                },
                {
                  "authentication": "pass"
                }
              ]
            },
            "type": "string"
          }
