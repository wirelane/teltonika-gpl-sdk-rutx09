{
  "openapi": "3.0.0",
  "paths": {
    "/ospf/global": {
      "get": {
        "summary": "Returns ospf global configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200OsGe"
          }
        },
        "tags": ["Global Settings"]
      },
      "put": {
        "summary": "Updates OSPF global configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200OsGe"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "description": "Parameter section",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "$ref": "#/components/schemas/ospf_general_options"
                  }
                }
              }
            }
          }
        },
        "tags": ["Global Settings"],
        "description": ""
      },
      "parameters": [],
      "x-web": {
        "menu": ["Network", "Routing", "Dynamic Routes", "OSPF"],
        "hw_tags": {
          "is_switch": false,
          "is_access_point": false
        },
        "name": "OSPF"
      },
      "post": {
        "summary": "Uploads custom OSPF configuration file",
        "operationId": "post-ospf-global-config-general",
        "responses": {
          "200": {
            "$ref": "#/components/responses/file_response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/ospfd_custom_conf"
        },
        "tags": ["Global Settings"]
      }
    },
    "/ospf/status": {
      "get": {
        "summary": "Fetches data about OSPF neighbors",
        "operationId": "get-network-routes-dynamic-ospf-general-actions-ospf_info",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "neighbors": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {}
                          }
                        },
                        "192.168.2.0/24": {
                          "type": "object",
                          "properties": {
                            "nexthops": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "directlyAttachedTo": {
                                    "type": "string"
                                  },
                                  "directly attached to": {
                                    "type": "string"
                                  },
                                  "ip": {
                                    "type": "string"
                                  }
                                }
                              }
                            },
                            "routeType": {
                              "type": "string"
                            },
                            "area": {
                              "type": "string"
                            },
                            "cost": {
                              "type": "integer"
                            }
                          }
                        }
                      }
                    }
                  },
                  "x-examples": {
                    "example-1": {
                      "data": {
                        "neighbors": [],
                        "192.168.2.0/24": {
                          "nexthops": [
                            {
                              "directlyAttachedTo": "br-lan",
                              "directly attached to": "br-lan",
                              "ip": " "
                            }
                          ],
                          "routeType": "N",
                          "area": "0.0.0.123",
                          "cost": 100
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["Global Settings"]
      },
      "parameters": [],
      "x-web": {
        "menu": ["Network", "Routing", "Dynamic Routes", "OSPF"],
        "hw_tags": {
          "is_switch": false,
          "is_access_point": false
        },
        "name": "OSPF"
      }
    }
  },
  "components": {
    "responses": {
      "200OsGe": {
        "description": "Returns section",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "enabled": {
                      "type": "string"
                    },
                    "enabled_vty": {
                      "type": "string"
                    },
                    "ospfd_custom_conf": {
                      "type": "string"
                    },
                    "ospfd_custom_conf:file_size": {
                      "type": "number"
                    },
                    "router_id": {
                      "type": "string"
                    },
                    "passive_ifname": {
                      "type": "string"
                    },
                    "originate": {
                      "type": "string"
                    },
                    "redistribute": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "file_response": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "path": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "ospf_general_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": "Enable Service",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enable/Disable OSPF protocol."
          },
          "enabled_vty": {
            "x-web": {
              "pretty": "Enable VTY",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enable/Disable vty access from LAN."
          },
          "ospfd_custom_conf": {
            "x-web": {
              "pretty": "Import Config",
              "input-type": "input"
            },
            "type": "string",
            "format": "binary",
            "description": "Use imported OSPF configuration."
          },
          "router_id": {
            "x-web": {
              "pretty": "Router ID",
              "input-type": "input"
            },
            "type": "string",
            "description": "IP address or any arbitrary 32bit number."
          },
          "passive_ifname": {
            "x-web": {
              "pretty": "Passive Interfaces",
              "input-type": "input"
            },
            "type": "array",
            "description": "OSPF hello packets are not sent on these interfaces.",
            "items": {
              "type": "string"
            }
          },
          "originate": {
            "x-web": {
              "pretty": "Generate a default external route",
              "input-type": "select",
              "enum-pretty": {
                "": "Off",
                "default": "Default",
                "always": "Always"
              }
            },
            "type": "string",
            "enum": ["default", "always"],
            "description": "Default - Advertises the default route if the route is in the route table. Always - Specifies to always advertise the default route regardless of whether the route table has a default route."
          },
          "redistribute": {
            "x-web": {
              "pretty": "Redistribution Options",
              "input-type": "input"
            },
            "type": "array",
            "description": "Route redistribution is a process that allows a network to use a routing protocol to dynamically route traffic based on information learned from a different routing protocol. Valid values are: Connected, Kernel, NHRP, BGP, RIP, EIGRP, Static or a custom value",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    "requestBodies": {
      "ospfd_custom_conf": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "option": {
                  "type": "string",
                  "enum": ["ospfd_custom_conf"],
                  "description": "Parameter to upload the file to."
                },
                "file": {
                  "type": "string",
                  "format": "binary",
                  "description": "File to upload."
                }
              }
            }
          }
        }
      }
    }
  },
  "info": {
    "title": "OSPF Global Settings",
    "version": "1.0",
    "description": "OSPF global settings api documentation"
  },
  "tags": [
    {
      "name": "Global Settings"
    }
  ]
}
