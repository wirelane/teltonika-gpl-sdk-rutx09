{
  "openapi": "3.0.0",
  "info": {
    "title": "VRF",
    "description": "VRF api documentation",
    "version": "1.0",
    "contact": {}
  },
  "paths": {
    "/vrf/config": {
      "get": {
        "tags": [
          "General"
        ],
        "summary": "Returns VRF configurations",
        "description": "Gets VRF configurations",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200vrfMulti"
          }
        },
        "operationId": "get-services-vrf-config"
      },
      "post": {
        "tags": [
          "General"
        ],
        "summary": "Creates VRF configuration",
        "responses": {
          "201": {
            "$ref": "#/components/responses/200vrf"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "operationId": "post-services-vrf-config",
        "description": "Creates VRF configuration",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "allOf": [
                      {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "description": "The ID of the VRF configuration",
                            "maxLength": 15,
                            "x-web": {
                              "pretty": "VRF name",
                              "input-type": "input"
                            }
                          }
                        },
                        "required": [
                          "id"
                        ]
                      },
                      {
                        "$ref": "#/components/schemas/vrf_options"
                      }
                    ]
                  }
                },
                "required": [
                  "data"
                ]
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "General"
        ],
        "summary": "Updates VRF configurations",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200vrfMulti"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "operationId": "put-services-vrf-config",
        "description": "Updates VRF configurations",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/req_body_vrf_multi"
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "General"
        ],
        "summary": "Deletes VRF configurations",
        "requestBody": {
          "description": "Parameter section",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "array",
                    "description": "Array of of section objects",
                    "items": {
                      "type": "string",
                      "description": "Section id"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/200deleted_multi"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          }
        },
        "operationId": "delete-services-vrf-config",
        "description": "Deletes VRF configurations"
      },
      "x-web": {
        "name": "VRF",
        "menu": [
          "Network",
          "VRF",
          "VRF"
        ],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false
        }
      }
    },
    "/vrf/config/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string"
          },
          "description": "The ID of the VRF configuration"
        }
      ],
      "get": {
        "tags": [
          "General"
        ],
        "summary": "Returns VRF configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200vrf"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          }
        },
        "operationId": "get-services-vrf-config-id",
        "description": "Gets VRF configuration"
      },
      "put": {
        "tags": [
          "General"
        ],
        "summary": "Updates VRF configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200vrf"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "operationId": "put-services-vrf-config-id",
        "description": "Updates VRF configuration",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/req_body_vrf_one"
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "General"
        ],
        "summary": "Deletes VRF configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200deleted"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          }
        },
        "operationId": "delete-services-vrf-config-id",
        "description": "Deletes VRF configuration"
      },
      "x-web": {
        "name": "VRF",
        "menu": [
          "Network",
          "VRF",
          "VRF"
        ],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false
        }
      }
    }
