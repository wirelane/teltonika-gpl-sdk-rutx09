  "components": {
    "responses": {
      "200vrf": {
        "description": "Returns section",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "allOf": [
                    {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "id"
                      ]
                    },
                    {
                      "$ref": "#/components/schemas/vrf_options"
                    }
                  ]
                }
              },
              "required": [
                "success",
                "data"
              ]
            }
          }
        }
      },
      "200vrfMulti": {
        "description": "Returns sections",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "array",
                  "uniqueItems": true,
                  "minItems": 1,
                  "items": {
                    "allOf": [
                      {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "id"
                        ]
                      },
                      {
                        "$ref": "#/components/schemas/vrf_options"
                      }
                    ]
                  }
                }
              },
              "required": [
                "success",
                "data"
              ]
            }
          }
        }
      }
    },
    "schemas": {
      "vrf_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "string",
            "x-web": {
              "pretty": "Enabled",
              "input-type": "switch"
            }
          },
          "table": {
            "type": "string",
            "description": "Unique routing table",
            "format": "irange(1, 4294967295)",
            "x-web": {
              "pretty": "Table",
              "input-type": "input"
            }
          },
          "link": {
            "type": "string",
            "description": "Devices which will be used for routing and forwarding",
            "x-web": {
              "pretty": "Link",
              "input-type": "multiselect"
            }
          }
        }
      },
      "req_body_vrf_one": {
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/vrf_options"
          }
        },
        "required": [
          "data"
        ]
      },
      "req_body_vrf_multi": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "allOf": [
                {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "The ID of the VRF configuration",
                      "x-web": {
                        "unique": true,
                        "pretty": "VRF name",
                        "input-type": "input"
                      }
                    }
                  },
                  "required": [
                    "id"
                  ]
                },
                {
                  "$ref": "#/components/schemas/vrf_options"
                }
              ]
            }
          }
        },
        "required": [
          "data"
        ]
      }
    }
  }
