{
    "services/usb_tools/general": {
        "description": "usb tools configuration",
        "read": {
            "api": [
                "/usb_tools/general/*"
            ]
        },
        "write": {
            "api": [
                "/usb_tools/general/*",
                "/administration/general/actions/reboot/*"
            ],
            "ubus": {
                "vuci.system": [
                    "reboot"
                ]
            }
        }
    }
}