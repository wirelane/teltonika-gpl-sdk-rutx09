    "schemas": {
      "troubleshoot_single_options": {
        "title": "troubleshoot_options",
        "type": "object",
        "properties": {
          "tcp_dump": {
            "x-web": {
              "pretty": "Enable TCP dump",
              "input-type": "switch",
              "hw_tags": {
                "is_access_point": false
              }
            },
            "type": "string",
            "format": "is_bool",
            "enum": [
              "0",
              "1"
            ],
            "description": "Turns TCP dump packet capture on or off."
          },
          "tcp_dump_interface": {
            "x-web": {
              "pretty": "Interface",
              "input-type": "input",
              "depends": [
                {
                  "tcp_dump": "1"
                }
              ],
              "hw_tags": {
                "is_access_point": false
              }
            },
            "type": "string",
            "description": "Only captures packets that move through the specified network interface."
          },
          "tcp_dump_filter": {
            "x-web": {
              "pretty": "Protocol filter",
              "input-type": "select",
              "depends": [
                {
                  "tcp_dump": "1"
                }
              ],
              "enum-pretty": {
				"": "All",
                "icmp": "ICMP",
                "tcp": "TCP",
                "udp": "UDP",
                "arp": "ARP"
              },
              "hw_tags": {
                "is_access_point": false
              }
            },
            "type": "string",
            "enum": [
			  "",
              "icmp",
              "tcp",
              "udp",
              "arp"
            ],
            "description": "Only captures packets that match the specified protocol."
          },
          "tcp_inout": {
            "x-web": {
              "pretty": "Packet direction",
              "input-type": "select",
              "depends": [
                {
                  "tcp_dump": "1"
                }
              ],
              "enum-pretty": {
                "inout": "Incoming/Outgoing",
                "in": "Incoming",
                "out": "Outgoing"
              },
              "hw_tags": {
                "is_access_point": false
              }
            },
            "type": "string",
            "enum": [
              "inout",
              "in",
              "out"
            ],
            "description": "Only captures packets coming from the specified direction."
          },
          "tcp_host": {
            "x-web": {
              "pretty": "Host",
              "input-type": "input",
              "depends": [
                {
                  "tcp_dump": "1"
                }
              ],
              "hw_tags": {
                "is_access_point": false
              }
            },
            "type": "string",
            "format": "host",
            "description": "Only captures packets related to the specified host."
          },
          "tcp_port": {
            "x-web": {
              "pretty": "Port",
              "input-type": "input",
              "depends": [
                {
                  "tcp_dump": "1"
                }
              ],
              "hw_tags": {
                "is_access_point": false
              }
            },
            "type": "string",
            "format": "port",
            "description": "Only captures packets related to the specified port."
          },
          "tcp_mount": {
            "x-web": {
              "pretty": "Storage",
              "input-type": "input",
              "depends": [
                {
                  "tcp_dump": "1"
                }
              ],
              "hw_tags": {
                "is_access_point": false
              }
            },
            "type": "string",
            "description": "Specifies where the TCP dump file will be stored."
          }
        }
      },
      "troubleshoot_options": {
        "allOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Troubleshoot Configuration identifier (ID).",
                "x-web": {
                  "pretty": "Configuration ID",
                  "input-type": "input"
                }
              }
            },
            "required": [
              "id"
            ]
          },
          {
            "$ref": "#/components/schemas/troubleshoot_single_options"
          }
        ]
      }
    }
