    "schemas": {
      "eigrp_general_options": {
        "type": "object",
        "x-examples": {
          "example-1": {
            "enabled": "0",
            "debug": "1",
            "as": "1",
            "router_id": "192.168.0.1",
            "network": "string",
            "redistribute": "string",
            "neighbor": "string"
          }
        },
        "properties": {
          "enabled": {
            "type": "string",
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            },
            "description": "Toggles EIGRP network ON or OFF.",
            "enum": [
              "0",
              "1"
            ]
          },
          "debug": {
            "type": "string",
            "x-web": {
              "pretty": "Enable logging",
              "input-type": "switch"
            },
            "description": "Enable logging of EIGRP.",
            "enum": [
              "1",
              "0"
            ]
          },
          "as": {
            "type": "string",
            "x-web": {
              "pretty": "AS",
              "input-type": "input"
            },
            "description": "EIGRP uses this number so that it makes sure it only talks to other EIGRP speakers that are in the same AS. For instance, if you have two routers, one with 'router eigrp 1' and one with 'router eigrp 2,' then they would not form an adjacency.",
            "format": "irange(1, 65535)"
          },
          "router_id": {
            "type": "string",
            "x-web": {
              "pretty": "Router ID",
              "input-type": "input"
            },
            "description": "EIGRP Router-ID in IP address format.",
            "format": "ipv4"
          },
          "network": {
            "type": "string",
            "x-web": {
              "pretty": "Network",
              "input-type": "input"
            },
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Add the announcement network.",
            "format": "ipmask4"
          },
            "redistribute": {
            "x-web": {
              "pretty": "Redistribution options",
              "input-type": "input"
            },
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Redistribute information from another routing protocol."
          },
          "neighbor": {
            "type": "string",
            "x-web": {
              "pretty": "Neighbors",
              "input-type": "input"
            },
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Add neighbor IP addresses.",
            "format": "ipmask4"
          }
        }
      }
    }
  }
