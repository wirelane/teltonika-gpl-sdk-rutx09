{
    "system/admin/access_control/general": {
        "description": "access control general",
        "read": {
            "file": {
                "/usr/lib/opkg/info/pam.control": [
                    "list"
                ]
            },
            "api": [
				"/access_control/ssh/*",
				"/access_control/cli/*",
				"/access_control/webui/*",
				"/access_control/telnet/*",
				"/certificates/config/*"
			]
        },
        "write": {
            "api": [
				"/access_control/ssh/*",
				"/access_control/cli/*",
				"/access_control/webui/*",
				"/access_control/telnet/*"
			]
        }
    },
    "system/admin/access_control/safety": {
        "description": "access control security",
        "read": {
			"api": ["/access_control/security/*"]
        },
        "write": {
			"api": ["/access_control/security/*"]
        }
    }
}