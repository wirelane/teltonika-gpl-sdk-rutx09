          "200": {
            "$ref": "#/components/responses/200_webui"
          }
        }
      },
      "put": {
        "summary": "Updates specified WebUI access control configurations",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200_webui"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/webui_multi"
        },
        "tags": ["Access Control WebUI"]
      },
      "x-web": {
        "menu": ["System", "Administration", "Access Control", "General"]
      }
    },
    "/access_control/webui/config/general": {
      "get": {
        "summary": "Returns the specified WebUI access control configuration",
        "tags": ["Access Control WebUI"],
        "responses": {
          "200": {
            "$ref": "#/components/responses/200_webui_one"
          }
        }
      },
      "put": {
        "summary": "Updates the specified WebUI access control configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200_webui_one"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/webui"
        },
        "tags": ["Access Control WebUI"]
      },
      "post": {
        "summary": "Upload file",
        "responses": {
          "200": {
            "$ref": "#/components/responses/file_response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/file_upload_ac_webui"
        },
        "tags": ["DNS"]
      },
      "x-web": {
        "menu": ["System", "Administration", "Access Control", "General"],
        "hw_tags": {
          "is_switch": false
        }
      }
    }
  },
