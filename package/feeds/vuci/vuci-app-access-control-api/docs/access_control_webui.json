    "schemas": {
      "webui": {
        "description": "",
        "type": "object",
        "x-examples": {
          "example-1": {
            "enable_http": "string",
            "enable_https": "string",
            "redirect_https": "string",
            "_httpWanAccess": "string",
            "listen_http": "string",
            "_httpsWanAccess": "string",
            "listen_https": "string",
            "rfc1918_filter": "string",
            "device_files": "string",
            "cert": "string",
            "key": "string"
          }
        },
        "properties": {
          "enable_http": {
            "x-web": {
              "pretty": "Enable HTTP access",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Turns HTTP access from the local network (LAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "enable_https": {
            "x-web": {
              "pretty": "Enable HTTPS access",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Turns HTTPS access from the local network (LAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "redirect_https": {
            "x-web": {
              "pretty": "Redirect to HTTPS",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Redirects connection attempts from HTTP to HTTPS.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "_httpWanAccess": {
            "x-web": {
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              },
              "pretty": "Enable remote HTTP access",
              "input-type": "switch",
              "depends": [
                {
                  "enable_http": "1"
                }
              ]
            },
            "type": "string",
            "description": "Turns HTTP access from remote networks (WAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "listen_http": {
            "x-web": {
              "pretty": "HTTP Port",
              "input-type": "input",
              "depends": [
                {
                  "enable_http": "1"
                }
              ]
            },
            "description": "Selects which port to use for HTTP access.",
            "type": "string",
            "format": "port"
          },
          "_httpsWanAccess": {
            "x-web": {
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              },
              "pretty": "Enable remote HTTPS access",
              "input-type": "switch",
              "depends": [
                {
                  "enable_https": "1"
                }
              ]
            },
            "type": "string",
            "description": "Turns HTTPS access from remote networks (WAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "listen_https": {
            "x-web": {
              "pretty": "HTTPS Port",
              "input-type": "input",
              "depends": [
                {
                  "enable_https": "1"
                }
              ]
            },
            "description": "Selects which port to use for HTTPS access.",
            "type": "string",
            "format": "port"
          },
          "rfc1918_filter": {
            "x-web": {
              "pretty": "Ignore private IPs on public interface",
              "input-type": "switch",
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              }
            },
            "type": "string",
            "description": "Prevent access from private (RFC1918) IPs on an interface if it has an public IP address.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "device_files": {
            "type": "string",
            "description": "Choose this option if you want to select certificate files from device.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "cert": {
            "x-web": {
              "pretty": "Server certificate",
              "input-type": "input",
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              }
            },
            "type": "string",
            "format": "path",
            "pattern": "^/etc/(vuci-uploads|certificates)/",
            "description": "Server certificate."
          },
          "key": {
            "x-web": {
              "pretty": "Server key",
              "input-type": "input",
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              }
            },
            "type": "string",
            "format": "path",
            "pattern": "^/etc/(vuci-uploads|certificates)/",
            "description": "Server key"
          }
        },
        "required": ["enable_http", "enable_https", "redirect_https", "listen_http", "listen_https"]
      }
    },
