{
  "openapi": "3.0.0",
  "tags": [
    {
      "name": "WebUI"
    }
  ],
  "paths": {
    "/access_control/webui/status": {
      "get": {
        "summary": "Returns all Access Control statuses",
        "tags": ["WebUI"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "string",
                      "description": "Operation status."
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "HTTP": {
                          "type": "object",
                          "properties": {
                            "lan": {
                              "type": "boolean",
                              "description": "HTTP on LAN is enabled/disabled."
                            },
                            "wan": {
                              "type": "boolean",
                              "description": "HTTP on WAN is enabled/disabled."
                            }
                          }
                        },
                        "HTTPS": {
                          "type": "object",
                          "properties": {
                            "lan": {
                              "type": "boolean",
                              "description": "HTTPS on LAN is enabled/disabled."
                            },
                            "wan": {
                              "type": "boolean",
                              "description": "HTTPS on WAN is enabled/disabled."
                            }
                          }
                        },
                        "SSH": {
                          "type": "object",
                          "properties": {
                            "lan": {
                              "type": "boolean",
                              "description": "SSH on LAN is enabled/disabled."
                            },
                            "wan": {
                              "type": "boolean",
                              "description": "SSH on WAN is enabled/disabled."
                            }
                          }
                        },
                        "CLI": {
                          "type": "object",
                          "properties": {
                            "lan": {
                              "type": "boolean",
                              "description": "CLI on LAN is enabled/disabled."
                            },
                            "wan": {
                              "type": "boolean",
                              "description": "CLI on WAN is enabled/disabled."
                            }
                          }
                        },
                        "Telnet": {
                          "type": "object",
                          "properties": {
                            "lan": {
                              "type": "boolean",
                              "description": "Telnet on LAN is enabled/disabled."
                            },
                            "wan": {
                              "type": "boolean",
                              "description": "Telnet on WAN is enabled/disabled."
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "x-web": {
        "menu": ["System", "Administration", "Access Control", "General"],
        "hw_tags": {
          "is_switch": false
        }
      }
    },
    "/access_control/webui/config": {
      "get": {
        "summary": "Returns WebUI configuration",
        "tags": ["WebUI"],
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlWebUI"
          }
        }
      },
      "put": {
        "summary": "Updates WebUI configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlWebUI"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/webui_request"
        },
        "tags": ["WebUI"]
      },
      "x-web": {
        "menu": ["System", "Administration", "Access Control", "General"]
      }
    },
    "/access_control/webui/config/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string"
          },
          "description": "Configuration ID"
        }
      ],
      "post": {
        "summary": "Uploads certificate or private key file",
        "responses": {
          "200": {
            "$ref": "#/components/responses/file_response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Configuration ID"
          }
        ],
        "requestBody": {
          "$ref": "#/components/requestBodies/file_upload_ac_webui"
        },
        "tags": ["WebUI"]
      },
      "get": {
        "summary": "Returns WebUI configuration",
        "tags": ["WebUI"],
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlWebUISingle"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          }
        }
      },
      "put": {
        "summary": "Updates WebUI configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlWebUISingle"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/webui_request_single"
        },
        "tags": ["WebUI"]
      },
      "x-web": {
        "menu": ["System", "Administration", "Access Control", "General"]
      }
    }
  },
  "components": {
    "responses": {
      "200AccessControlWebUI": {
        "description": "Returns configuration",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "array",
                  "description": "Array of section objects",
                  "items": {
                    "$ref": "#/components/schemas/access_control_webui_options_response"
                  }
                }
              }
            }
          }
        }
      },
      "200AccessControlWebUISingle": {
        "description": "Returns configuration",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "$ref": "#/components/schemas/access_control_webui_options_response"
                }
              }
            }
          }
        }
      },
      "file_response": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "path": {
                      "type": "string"
                    }
                  }
                }
              },
              "x-examples": {
                "example-1": {
                  "success": true,
                  "data": {
                    "path": "/etc/vuci-uploads/cbid.file.id.parameter.file_name.ext"
                  }
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "access_control_webui_options_response": {
        "allOf": [
          {
            "$ref": "#/components/schemas/access_control_webui_options"
          },
          {
            "type": "object",
            "properties": {
              "key:file_size": {
                "type": "string",
                "description": "Uploaded certificate key file size"
              },
              "cert:file_size": {
                "type": "string",
                "description": "Uploaded certificate file size"
              }
            }
          }
        ]
      },
      "access_control_webui_options": {
        "allOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Configuration ID"
              }
            }
          },
          {
            "$ref": "#/components/schemas/access_control_webui_single_options"
          }
        ]
      },
      "access_control_webui_single_options": {
        "type": "object",
        "properties": {
          "enable_http": {
            "x-web": {
              "pretty": "Enable HTTP access",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Turns HTTP access from the local network (LAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "enable_https": {
            "x-web": {
              "pretty": "Enable HTTPS access",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Turns HTTPS access from the local network (LAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "redirect_https": {
            "x-web": {
              "pretty": "Redirect to HTTPS",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Redirects connection attempts from HTTP to HTTPS.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "http_wan_access": {
            "x-web": {
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              },
              "pretty": "Enable remote HTTP access",
              "input-type": "switch",
              "depends": [
                {
                  "enable_http": "1"
                }
              ]
            },
            "type": "string",
            "description": "Turns HTTP access from remote networks (WAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "listen_http": {
            "x-web": {
              "pretty": "HTTP Port",
              "input-type": "input",
              "depends": [
                {
                  "enable_http": "1"
                }
              ]
            },
            "description": "Selects which port to use for HTTP access.",
            "type": "string",
            "format": "port"
          },
          "https_wan_access": {
            "x-web": {
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              },
              "pretty": "Enable remote HTTPS access",
              "input-type": "switch",
              "depends": [
                {
                  "enable_https": "1"
                }
              ]
            },
            "type": "string",
            "description": "Turns HTTPS access from remote networks (WAN) to the router's WebUI on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "listen_https": {
            "x-web": {
              "pretty": "HTTPS Port",
              "input-type": "input",
              "depends": [
                {
                  "enable_https": "1"
                }
              ]
            },
            "description": "Selects which port to use for HTTPS access.",
            "type": "string",
            "format": "port"
          },
          "rfc1918_filter": {
            "x-web": {
              "pretty": "Ignore private IPs on public interface",
              "input-type": "switch",
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              }
            },
            "type": "string",
            "description": "Prevent access from private (RFC1918) IPs on an interface if it has an public IP address.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "device_files": {
            "type": "string",
            "description": "Choose this option if you want to select certificate files from device.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "cert": {
            "x-web": {
              "pretty": "Server certificate",
              "input-type": "input",
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              }
            },
            "type": "string",
            "format": "path",
            "pattern": "^/etc/(vuci-uploads|certificates)/",
            "description": "Server certificate."
          },
          "key": {
            "x-web": {
              "pretty": "Server key",
              "input-type": "input",
              "hw_tags": {
                "is_access_point": false,
                "is_switch": false
              }
            },
            "type": "string",
            "format": "path",
            "pattern": "^/etc/(vuci-uploads|certificates)/",
            "description": "Server key"
          }
        },
        "required": ["enable_http", "enable_https", "redirect_https", "listen_http", "listen_https"]
      }
    },
    "requestBodies": {
      "webui_request": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "description": "Array of section objects",
                  "items": {
                    "$ref": "#/components/schemas/access_control_webui_options"
                  }
                }
              },
              "required": ["data"]
            }
          }
        }
      },
      "webui_request_single": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "$ref": "#/components/schemas/access_control_webui_single_options"
                }
              },
              "required": ["data"]
            }
          }
        }
      },
      "file_upload_ac_webui": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "option": {
                  "type": "string",
                  "description": "Parameter to upload the file to.",
                  "enum": ["cert", "key"]
                },
                "file": {
                  "type": "string",
                  "format": "binary",
                  "description": "File to upload."
                }
              },
              "required": ["option", "file"]
            }
          }
        }
      }
    }
  }
}
