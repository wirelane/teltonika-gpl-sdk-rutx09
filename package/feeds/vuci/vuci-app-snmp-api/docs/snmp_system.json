          }
        },
        "operationId": "snmp1"
      },
      "put": {
        "tags": [
          "SNMP system"
        ],
        "summary": "Updates the general SNMP configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200snmp_system"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "description": "Data to update configuration",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
