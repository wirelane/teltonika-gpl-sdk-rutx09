          "200": {
            "$ref": "#/components/responses/200snmp_system_multi"
          }
        },
        "operationId": "snmp3"
      },
      "put": {
        "tags": [
          "SNMP system"
        ],
        "summary": "Updates the general SNMP configuration in an array",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200snmp_system_multi"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "description": "Data to update configurations",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "array",
                    "description": "Array of configurations to update",
                    "items": {
                      "$ref": "#/components/schemas/snmp_system_options_id"
                    }
                  }
                }
              }
            }
          }
        },
        "operationId": "snmp4"
