  "components": {
    "responses": {
      "200snmp_system": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status."
                },
                "data": {
                  "$ref": "#/components/schemas/snmp_system_options_id"
                }
              }
            }
          }
        }
      },
      "200snmp_system_multi": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status."
                },
                "data": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/snmp_system_options_id"
                  }
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "snmp_system_options": {
        "type": "object",
        "properties": {
          "oid": {
            "type": "string",
            "description": "An OID is an Object Identifier. It's an address used to identify devices and their statuses.",
            "readOnly": true,
            "x-web": {
              "pretty": "System OID"
            }
          },
          "sysLocation": {
            "type": "string",
            "description": "Location of the system. If the field is empty, the option will have read-write permissions. If not, the option will become read-only.",
            "maxLength": 255,
            "format": "string",
            "x-web": {
              "pretty": "Location",
              "input-type": "input"
            }
          },
          "sysContact": {
            "type": "string",
            "description": "Contact email. If the field is empty, the option will have read-write permissions. If not, the option will become read-only.",
            "maxLength": 255,
            "format": "string",
            "x-web": {
              "pretty": "Contact",
              "input-type": "input"
            }
          },
          "sysName": {
            "type": "string",
            "description": "Name of the system. If the field is empty, the option will have read-write permissions. If not, the option will become read-only.",
            "maxLength": 255,
            "format": "string",
            "x-web": {
              "pretty": "Name",
              "input-type": "input"
            }
          }
        }
      },
      "snmp_system_options_id": {
        "allOf": [
          {
            "$ref": "#/components/schemas/snmp_system_options"
          },
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "SNMP System Configuration identifier (ID)."
              }
            },
            "required": [
              "id"
            ]
          }
        ],
        "title": ""
      }
    }
  }
