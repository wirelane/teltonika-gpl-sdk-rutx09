    "schemas": {
      "snmp_trap_settings": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "string",
            "description": "Enable",
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            }
          },
          "host": {
            "type": "string",
            "description": "Host to transfer SNMP (Simple Network Management Protocol) traffic to.",
            "format": "host",
            "x-web": {
              "pretty": "Host",
              "input-type": "input"
            }
          },
          "port": {
            "type": "string",
            "description": "Port for trap host.",
            "format": "port",
            "default": "162",
            "x-web": {
              "pretty": "Port",
              "input-type": "input"
            }
          },
          "community": {
            "type": "string",
            "description": "The SNMP (Simple Network Management Protocol) community is an ID that allows access to a routers SNMP data.",
            "format": "uciname",
            "maxLength": 16,
            "default": "Public",
            "x-web": {
              "pretty": "Community",
              "input-type": "input"
            }
          }
        }
      },
