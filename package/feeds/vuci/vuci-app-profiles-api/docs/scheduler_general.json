          }
        },
        "tags": [
          "Profiles Scheduler General"
        ]
      },
      "put": {
        "summary": "Updates specified scheduler general configurations",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200schgenMulti"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "array",
                    "description": "array of of section objects",
                    "items": {
                      "$ref": "#/components/schemas/scheduler_general_options"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Profiles Scheduler General"
        ]
      },
      "x-web": {
        "menu": [
          "System",
          "Administration",
          "Profiles",
          "Scheduler"
