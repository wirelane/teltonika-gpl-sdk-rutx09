        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "$ref": "#/components/schemas/scheduler_general_options_single"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Profiles Scheduler General"
        ]
      },
      "x-web": {
        "menu": [
          "System",
          "Administration",
          "Profiles",
          "Scheduler"
        ]
      }
