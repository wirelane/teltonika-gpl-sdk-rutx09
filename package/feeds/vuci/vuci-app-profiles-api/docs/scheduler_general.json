        "responses": {
          "200": {
            "$ref": "#/components/responses/200schgen"
          }
        },
        "tags": [
          "Profiles Scheduler General"
        ]
      },
      "put": {
        "summary": "Updates the specified scheduler general configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200schgen"
