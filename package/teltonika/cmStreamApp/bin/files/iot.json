{
	"user": "iot",
	"publish": [ "iot.*" ],
	"access": {
		"gsm": {
			"methods": [
				"info"
			]
		},
		"gsm.modem*": {
			"methods": [
				"info",
				"get_service_provider",
				"get_signal_query",
				"get_net_reg_stat_all",
				"get_network_info",
				"get_iccid"
			]
		},
		"rpc-sys": {
			"methods": [
				"reboot"
			]
		},
		"network.interface": {
			"methods": [
				"dump"
			]
		},
		"system": {
			"methods": [
				"sysupgrade",
				"backup",
				"validate_firmware_image"
			]
		},
		"log": {
			"methods": [
				"write_ext"
			]
		},
		"file": {
			"methods": [
				"exec"
			]
		},
		"wan_info": {
			"methods": [
				"get"
			]
		}
	}
}
