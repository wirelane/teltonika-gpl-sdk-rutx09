{
	"user": "urcd",
	"subscribe": [ "gsm.modem*" ],
	"listen": [ "ubus.object.remove", "ubus.object.add", "sim.changed" ],
	"publish": [ "urcd.modem*" ],
	"access": {
		"gsm": {
			"methods": [ "info" ]
		},
		"gsm.modem*": {
			"methods": [ "info", "read_sms", "delete_sms", "get_sim_slot" ]
		},
		"ip_block": {
			"methods": [ "show" ]
		}
	}
}
