{
	"user": "juggler",
	"publish": [
		"event_juggler*"
	],
	"listen": [
		"ubus.object.remove",
		"ubus.object.add",
		"quota.limit_reach",
		"logd.db_write",
		"chilli.*"
	],
	"subscribe": [
		"ioman.*",
		"gsm.modem*",
		"gpsd"
	],
	"access": {
		"gsm": {
			"methods": [
				"info"
			]
		},
		"gsm.modem*": {
			"methods": [
				"info",
				"get_service_provider",
				"get_pin_state",
				"get_sim_slot",
				"get_iccid",
				"get_signal_query",
				"get_network_info",
				"get_net_reg_stat_all",
				"get_ca_info",
				"get_serving_cell",
				"get_imsi",
				"get_neighbour_cells",
				"change_sim_slot",
				"send_sms"
			]
		},
		"ioman.*": {
			"methods": [
				"status",
				"location",
				"update",
				"invert_value"
			]
		},
		"gpsd": {
			"methods": [
				"position"
			]
		},
		"mobifd": {
			"methods": [
				"reload"
			]
		},
		"mobifd.modem*": {
			"methods": [
				"reload"
			]
		},
		"network": {
			"methods": [
				"reload"
			]
		},
		"network.interface": {
			"methods": [
				"up",
				"down",
				"dump"
			]
		},
		"data_sender.*": {
			"methods": [
				"send"
			]
		},
		"system": {
			"methods": [
				"change_profile"
			]
		},
		"port_events": {
			"methods": [
				"set_topo_ev"
			]
		},
		"uci" : {
			"methods": [
				"reload_config"
			]
		},
		"sys": {
			"methods": [
				"reboot",
				"poweroff"
			]
		},
		"rut_fota" : {
			"methods": [
				"get_info"
			]
		},
		"rms" : {
			"methods": [
				"get_status"
			]
		}
	}
}
